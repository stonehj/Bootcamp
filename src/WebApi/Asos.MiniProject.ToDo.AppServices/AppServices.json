{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appServiceName": {
      "defaultValue": "asbc-as-todo-eun-teamname",
      "type": "string"
    },
    "appServicePlanName": {
      "defaultValue": "asbc-asp-todo-eun-teamname",
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Web/serverfarms",
      "sku": {
        "name": "B1",
        "capacity": 1
      },
      "kind": "app",
      "name": "[toLower(parameters('appServicePlanName'))]",
      "apiVersion": "2016-09-01",
      "location": "North Europe",
      "tags": {
        "platform": "emergenttalent2017"
      },
      "properties": {
        "name": "[toLower(parameters('appServicePlanName'))]"
      },
      "dependsOn": []
    },
    {
      "type": "Microsoft.Web/sites",
      "kind": "api",
      "name": "[toLower(parameters('appServiceName'))]",
      "apiVersion": "2016-08-01",
      "location": "North Europe",
      "tags": {
        "platform": "emergenttalent2017"
      },
      "properties": {
        "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', toLower(parameters('appServicePlanName')))]"
      },
      "dependsOn": [
        "[resourceId('Microsoft.Web/serverfarms', toLower(parameters('appServicePlanName')))]"
      ]
    }
  ]
}